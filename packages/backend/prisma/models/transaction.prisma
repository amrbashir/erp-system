model Transaction {
  id Int @id @default(autoincrement())

  amount Int

  invoice Invoice?
  expense Expense?

  createdAt DateTime @default(now())

  cashier   User @relation(fields: [cashierId], references: [id])
  cashierId Int

  store   Store? @relation(fields: [storeId], references: [id])
  storeId Int?

  @@map("transactions")
}
