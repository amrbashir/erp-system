{
  "name": "@tech-zone-store/backend",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "type": "module",
  "main": "dist/main.js",
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch --config tsdown.config.dev.ts",
    "start": "node --env-file=.env dist/main.js",
    "start:dev": "node --env-file=.env.dev dist/main.js",
    "pretest": "prisma generate && docker compose up -d",
    "test": "vitest run",
    "test:watch": "vitest",
    "prets:check": "prisma generate",
    "ts:check": "tsc --noEmit"
  },
  "dependencies": {
    "@nestjs/common": "^11.1.2",
    "@nestjs/core": "^11.1.2",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.2",
    "@prisma/client": "^6.8.2",
    "argon2": "^0.43.0",
    "class-transformer": "^0.5.1",
    "express": "^5.1.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.2.2"
  },
  "devDependencies": {
    "@nestjs/testing": "^11.1.2",
    "@types/express": "^5.0.2",
    "@types/node": "^22.15.29",
    "@types/passport": "^1.0.17",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "dotenv": "^16.5.0",
    "prisma": "^6.8.2",
    "tsdown": "^0.12.6",
    "typescript": "^5.8.3",
    "unplugin-swc": "^1.5.4",
    "vite": "^6.3.5",
    "vitest": "^3.1.4"
  }
}
